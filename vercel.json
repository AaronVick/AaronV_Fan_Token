{
  "version": 2,
  "functions": {
    "src/index.ts": {
      "runtime": "@vercel/node@20.x"
    }
  },
  "builds": [
    { "src": "src/index.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/src/index.ts" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "text/html; charset=utf-8" },
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ]
}
