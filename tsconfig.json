{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*", "manual_modules/*"]
    }
  },
  "include": [
    "*.ts",
    "manual_modules/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
